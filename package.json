{
  "name": "@blb-ventures/react-hooks",
  "version": "0.0.0-development",
  "description": "",
  "main": "./lib/cjs/index.js",
  "files": [
    "lib/**/*"
  ],
  "homepage": "https://github.com/blb-ventures/react-hooks",
  "repository": {
    "type": "git",
    "url": "https://github.com/blb-ventures/react-hooks.git"
  },
  "bugs": {
    "url": "https://github.com/blb-ventures/react-hooks/issues"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "prepack": "npm run build",
    "lint": "eslint ./src",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "react",
    "react-hooks"
  ],
  "author": "Eduardo S Ciciliato",
  "license": "ISC",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "devDependencies": {
    "@types/eslint": "^8.21.0",
    "@types/react": "^18.0.27",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "confusing-browser-globals": "^1.0.11",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "react": "^18.2.0",
    "semantic-release": "^20.1.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "contributors": [
    {
      "name": "Eduardo S Ciciliato",
      "url": "https://eduardociciliato.com.br"
    }
  ],
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
